# Anthropic Claude API Key - get from https://console.anthropic.com/
ANTHROPIC_API_KEY=your_api_key_here

# API runs on this port (Render sets PORT automatically)
PORT=3000

# Admin panel - password for /admin (change in production!)
ADMIN_PASSWORD=your_admin_password

# License validation - "strict" = only approved licenses, "skip" = bypass (dev)
LICENSE_MODE=strict
